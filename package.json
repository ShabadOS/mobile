{
  "name": "@shabados/mobile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --color",
    "tsc:watch": "tsc -w",
    "emulator:android": "react-native run-android",
    "emulator:ios": "react-native run-ios --simulator='iPhone 8'",
    "start:android": "run-p tsc:watch emulator:android",
    "start:ios": "run-p tsc:watch emulator:ios",
    "clean:ios": "cd ios && rimraf Pods && xcodebuild clean",
    "clean:android": "run-script-os",
    "clean:android:default": "cd android && ./gradlew clean",
    "clean:android:windows": "cd android && gradlew clean",
    "start": "react-native start",
    "test": "jest --color --watch",
    "install-assets": "react-native link",
    "postinstall": "pod-install ios"
  },
  "dependencies": {
    "@nozbe/watermelondb": "^0.19.0",
    "@nozbe/with-observables": "^1.0.5",
    "@react-native-community/toolbar-android": "0.1.0-rc.2",
    "@react-navigation/drawer": "^5.8.5",
    "@react-navigation/material-bottom-tabs": "^5.2.13",
    "@react-navigation/native": "^5.7.0",
    "@react-navigation/stack": "^5.7.0",
    "react": "16.13.1",
    "react-native": "0.63.2",
    "react-native-gesture-handler": "^1.6.0",
    "react-native-reanimated": "^1.9.0",
    "react-native-safe-area-context": "^0.7.3",
    "react-native-screens": "^2.9.0",
    "react-native-vector-icons": "^6.7.0",
    "reanimated-bottom-sheet": "^1.0.0-alpha.22",
    "rxjs": "^6.6.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.10.4",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@react-native-community/eslint-config": "^1.1.0",
    "@types/jest": "^25.2.3",
    "@types/react": "^16.9.42",
    "@types/react-native": "^0.63.17",
    "@types/react-native-vector-icons": "^6.4.5",
    "@types/react-test-renderer": "^16.9.3",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "babel-jest": "^25.5.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "husky": "^4.2.5",
    "jest": "^25.5.4",
    "jetifier": "^1.6.6",
    "metro-react-native-babel-preset": "^0.59.0",
    "npm-run-all": "^4.1.5",
    "pod-install": "^0.1.10",
    "prettier": "2.1.1",
    "react-native-testing-library": "^1.14.0",
    "react-test-renderer": "^16.13.1",
    "rimraf": "^3.0.2",
    "run-script-os": "^1.1.1",
    "ts-jest": "^25.5.1",
    "typescript": "^3.9.6"
  }
}
